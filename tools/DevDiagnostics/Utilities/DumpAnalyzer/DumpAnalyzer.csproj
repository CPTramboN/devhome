<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(SolutionDir)ToolingVersions.props" />

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RuntimeIdentifiers>win-x86;win-x64;win-arm64</RuntimeIdentifiers>
    <PublishProfileFullPath Condition="'$(BuildingInsideVisualStudio)' != 'True'">$(SolutionDir)\src\Properties\PublishProfiles\win-$(Platform).pubxml</PublishProfileFullPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Platform)'=='x64'">
    <AnalysisNugetPlatform>x64</AnalysisNugetPlatform>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Platform)'!='x64'">
    <AnalysisNugetPlatform>$(Platform)</AnalysisNugetPlatform>
  </PropertyGroup>

  <ItemGroup>
    <!-- CLRComposition files-->
    <None Include="CLRComposition\$(ProcessorArchitecture)\CLRComposition.dll" Link="CLRComposition\CLRComposition.dll">
      <TargetPath>winext\%(Filename)%(Extension)</TargetPath>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>


  <ItemGroup>
    <Compile Remove="definitions\**" />
    <EmbeddedResource Remove="definitions\**" />
    <None Remove="definitions\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Debugging.Platform.DbgEng" Version="20240722.1449.0" GeneratePathProperty="true" />
    <PackageReference Include="Microsoft.Debugging.Platform.SrcSrv" Version="20240722.1449.0" GeneratePathProperty="true" />
    <PackageReference Include="Microsoft.Debugging.Platform.SymSrv" Version="20240722.1449.0" GeneratePathProperty="true" />
    <PackageReference Include="Microsoft.Diagnostics.Analysis.Ext" Version="10.240607.233526-REL-2406-32" GeneratePathProperty="true">
      <IncludeAssets>none</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Diagnostics.DbgShim.win-arm64" Version="8.0.532401" GeneratePathProperty="true">
      <IncludeAssets>none</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Diagnostics.DbgShim.win-x64" Version="8.0.532401" GeneratePathProperty="true">
